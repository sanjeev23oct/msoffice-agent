{
  "name": "outlook-onenote-ai-agent",
  "version": "1.0.0",
  "description": "AI Agent for Outlook and OneNote integration",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:server\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite",
    "dev:server": "tsx watch src/server/index.ts",
    "test:server": "tsx src/server/index.ts",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron:wait": "wait-on http://localhost:5173 http://localhost:3001 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:main && npm run build:renderer && npm run build:server",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "electron .",
    "package": "electron-builder",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "electron",
    "outlook",
    "onenote",
    "ai",
    "copilotkit"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-node": "^2.6.0",
    "@copilotkit/react-core": "^1.0.0",
    "@copilotkit/react-ui": "^1.0.0",
    "@copilotkit/runtime": "^1.0.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "openai": "^4.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "sql.js": "^1.10.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-electron": "^0.28.2",
    "wait-on": "^9.0.1"
  }
}
